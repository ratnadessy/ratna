  <h1>üéÅ CAGARBOLA üéÅ</h1>
  <div id="promo" bis_skin_checked="1">Cuma isi deposit pakai kode L6L35Z, langsung dapet Rp193.870 di <a href="https://cagarniat.ink" target="_blank">cagarniat.ink</a>. Top up QRIS cuma 10rb, langsung gas cuan!</div>

  <button onclick="generatePromo()">Generate</button>

  <div bis_skin_checked="1">
    <input type="tel" id="phoneNumber" placeholder="Masukkan nomor HP (628xxxx)">
    <button onclick="sendToWhatsApp()">Kirim ke WhatsApp</button>
    <button oneclick="sendToCOPY()">COPY</button>
  </div>

  <script nonce="MzUyMTAxNjYzMSwyMTU1MzI4NDA0">
  const links = [
    "cagarpintar.com",
    "cagaringat.ink",
    "cagarniat.ink",
    "cagarberani.ink"
  ];

  const templates = [
    "Mau dapat Rp{amount}? Gampang banget! Masuk ke {link} dan pakai kode {code}.",
    "Kode spesial {code} bisa bikin kamu dapet Rp{amount} cuma-cuma. Coba langsung di {link}!",
    "Cuma isi deposit pakai kode {code}, langsung dapet Rp{amount} di {link}.",
    "Khusus pengguna cepat! Masukkan kode {code} & claim Rp{amount} di {link} sekarang juga!",
    "Gak percaya? Masuk ke {link} dan buktiin sendiri! Pakai kode {code} buat dapetin Rp{amount}.",
    "Gak perlu ribet. Deposit pake kode {code} dan cuan Rp{amount} bisa langsung kamu klaim di {link}!"
  ];

  const extras = [
    "Deposit QRIS hanya 10.000, proses hanya 1 detik!",
    "Minimal deposit cuma 10rb via QRIS, langsung masuk!",
    "Proses kilat! QRIS cuma 10.000 udah bisa main.",
    "QRIS tersedia, cukup 10 ribu dan instan masuk!",
    "Top up QRIS cuma 10rb, langsung gas cuan!"
  ];

  let lastPlainText = "";

  function getRandomCode() {
    const letters = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
    const numbers = "0123456789";
    let chars = [];

    for (let i = 0; i < 3; i++) chars.push(letters[Math.floor(Math.random() * letters.length)]);
    for (let i = 0; i < 3; i++) chars.push(numbers[Math.floor(Math.random() * numbers.length)]);

    for (let i = chars.length - 1; i > 0; i--) {
      const j = Math.floor(Math.random() * (i + 1));
      [chars[i], chars[j]] = [chars[j], chars[i]];
    }

    return chars.join('');
  }

  function getRandomAmount() {
    return Math.floor(Math.random() * (250000 - 70000 + 1)) + 70000;
  }

  function formatRupiah(angka) {
    return angka.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".");
  }

  function generatePromo() {
    const code = getRandomCode();
    const amount = formatRupiah(getRandomAmount());
    const link = links[Math.floor(Math.random() * links.length)];
    const template = templates[Math.floor(Math.random() * templates.length)];
    const extra = extras[Math.floor(Math.random() * extras.length)];

    const plain = `${template
      .replace(/{code}/g, code)
      .replace(/{amount}/g, amount)
      .replace(/{link}/g, link)} ${extra}`;

    lastPlainText = plain;

    const htmlWithLink = plain.replace(
      new RegExp(link, 'g'),
      `<a href="https://${link}" target="_blank">${link}</a>`
    );

    document.getElementById("promo").innerHTML = htmlWithLink;
  }

  function sendToWhatsApp() {
    const phone = document.getElementById("phoneNumber").value.trim();
    if (!lastPlainText) {
      alert("Silakan generate promo dulu!");
      return;
    }
    if (!phone || !/^628[0-9]{8,}$/.test(phone)) {
      alert("Masukkan nomor HP valid dengan format 628xxxx...");
      return;
    }

    const encodedMessage = encodeURIComponent(lastPlainText);
    const waUrl = `https://wa.me/${phone}?text=${encodedMessage}`;
    window.open(waUrl, '_blank');
  }

  // Auto-generate promo saat halaman dibuka
  generatePromo();
  </script>


</body></html>
